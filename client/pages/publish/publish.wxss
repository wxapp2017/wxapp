.gs-signup {
  width: 100%;
  min-height: 100vh;
  margin: 0 auto;
  background-color: #eee;
  overflow: hidden;
}
/*.signup-title {
  width: 100%;
  line-height: 120rpx;
  padding-left: 30rpx;
  background-color: #1aad19;
  color: #fff;
}*/
.signup-form {
  display: block;
  margin: 0 auto;
  width: 90%;
  padding-top: 30rpx;
  overflow: hidden;
}
.signup-section {
  display: flex;
  line-height: 70rpx;
  margin-bottom: 30rpx;
  overflow: hidden;
  /*background-color: #fff;*/
}
.signup-item {
  width: 30%;
  text-align: right;
  padding-right: 10rpx;
  color: #555;
}
.signup-singleline {
  margin-left: 2%;
  width: 68%;
  height: 70rpx;
  line-height: 70rpx;
  padding-left: 10rpx;
  border-radius: 5rpx;
  background-color: #fff;
  color: #999;
}
.signup-multiline {
  margin-left: 2%;
  width: 68%;
  line-height: 60rpx;
  padding-left: 10rpx;
  border-radius: 5rpx;
  background-color: #fff;
  color: #999;
}
.signup-radiogroup {
  background-color: #eee;
}
.signup-checkboxgroup {
  background-color: #eee;
}
.signup-label {
  display: block;
}
.signup-textarea {
  height: 180rpx;
  line-height: 60rpx;
}
.signup-btn {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #1aad19;
  color: #fff;
}
.button-hover {
  background-color: #eee;
}
.signup-switch {
  height: 67rpx;
}
.signup-slider {
  margin: 0;
  padding-left: 40rpx;
  background-color: #eee;
}

